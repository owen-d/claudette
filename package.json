{
  "name": "claudette",
  "displayName": "claudette",
  "description": "claude powered editor integration, DIY",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claudette.generateWith20LinesContext",
        "title": "Claudette: Generate at cursor with 20 Lines Context"
      },
      {
        "command": "claudette.generateWith50LinesContext",
        "title": "Claudette: Generate at cursor with 50 Lines Context"
      },
      {
        "command": "claudette.generateWith100LinesContext",
        "title": "Claudette: Generate at cursor with 100 Lines Context"
      },
      {
        "command": "claudette.generateWithAllLinesContext",
        "title": "Claudette: Generate at cursor with All Lines Context"
      },
      {
        "command": "claudette.refactorWith20LinesContext",
        "title": "Claudette: Refactor selection with 20 Lines Context"
      },
      {
        "command": "claudette.refactorWith50LinesContext",
        "title": "Claudette: Refactor selection with 50 Lines Context"
      },
      {
        "command": "claudette.refactorWith100LinesContext",
        "title": "Claudette: Refactor selection with 100 Lines Context"
      },
      {
        "command": "claudette.refactorWithAllLinesContext",
        "title": "Claudette: Refactor selection with All Lines Context"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.26.0",
    "axios": "^1.7.4"
  }
}