{
  "name": "claudette",
  "repository": "github:owen-d/claudette",
  "displayName": "claudette",
  "description": "claude powered editor integration, DIY",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claudette.cursor",
        "shortTitle": "cursor",
        "title": "Claudette: Complete at cursor",
        "key": "C-l c c"
      },
      {
        "command": "claudette.comment",
        "shortTitle": "comment",
        "title": "Claudette: Complete comment at cursor",
        "key": "C-l c shift+c"
      },
      {
        "command": "claudette.refactor",
        "shortTitle": "refactor",
        "title": "Claudette: refactor selection",
        "key": "C-l c r"
      },
      {
        "command": "claudette.fix",
        "shortTitle": "fix",
        "title": "Claudette: fix next problem",
        "key": "C-l c f"
      },
      {
        "command": "claudette.findTSDefinitions",
        "shortTitle": "ts defs",
        "title": "Claudette: find ts defs"
      },
      {
        "command": "claudette.findGoDefinitions",
        "shortTitle": "go defs",
        "title": "Claudette: find go defs"
      },
      {
        "command": "claudette.refs",
        "shortTitle": "refs",
        "title": "Claudette: find references"
      },
      {
        "command": "claudette.wip",
        "shortTitle": "wip",
        "title": "Claudette: work in progress",
        "key": "C-l c w"
      },
      {
        "command": "claudette.repeat",
        "shortTitle": "repeat",
        "title": "Claudette: repeat last command",
        "key": "C-l c ."
      }
    ],
    "keybindings": [
      {
        "command": "claudette.cursor",
        "key": "ctrl+l c c",
        "when": "editorTextFocus"
      },
      {
        "command": "claudette.comment",
        "key": "ctrl+l c shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "claudette.refactor",
        "key": "ctrl+l c r",
        "when": "editorTextFocus"
      },
      {
        "command": "claudette.fix",
        "key": "ctrl+l c f",
        "when": "editorTextFocus"
      },
      {
        "command": "claudette.wip",
        "key": "ctrl+l c w",
        "when": "editorTextFocus"
      },
      {
        "command": "claudette.repeat",
        "key": "ctrl+l c .",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "vsc-test": "vscode-test",
    "pkg": "npx vsce package",
    "publish": "npx vsce package && code --install-extension ./claudette-0.0.1.vsix"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.26.0",
    "axios": "^1.7.4",
    "typescript": "^5.5.4"
  }
}